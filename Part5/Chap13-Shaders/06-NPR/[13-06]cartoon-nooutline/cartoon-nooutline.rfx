    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matViewProjection;
float4x4 matView;
float3 vLight;

struct VS_INPUT 
{
   float4 Position : POSITION;
   float3 Normal   : NORMAL;
};

struct VS_OUTPUT 
{
   float4 Position : POSITION;
   float2 Texcoord : TEXCOORD0;
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position = mul( Input.Position, matViewProjection );
   float3 N = normalize( mul( Input.Normal, matView ) );
   float3 L = normalize( -vLight );
   float diffuse = max( 0, dot( N, L ) );
   Output.Texcoord = float2( diffuse, 0.5f );

   return( Output );
   
}



]]></RmShaderCode>
