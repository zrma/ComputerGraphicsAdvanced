    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matViewProjection;
float4x4 matView;
float3   vLight;

struct VS_INPUT
{
   float4 Pos      : POSITION;
   float3 Normal   : NORMAL;
   float2 uv       : TEXCOORD0;
};

struct VS_OUTPUT
{
   float4 Pos      : POSITION;
   float3 uvw      : TEXCOORD0;
};

VS_OUTPUT vs_main( VS_INPUT In )
{
   VS_OUTPUT Out = (VS_OUTPUT) 0; 
   Out.Pos = mul( In.Pos, matViewProjection );
   float3 N = normalize( mul( In.Normal, matView ) );
   float3 L = normalize( vLight );
   float diffuse = max( 0, dot( N, L ) );
   diffuse = pow( diffuse, 4 );
   Out.uvw.xy = In.uv;
   Out.uvw.z = 1 - diffuse;
   
   return Out;
}]]></RmShaderCode>
