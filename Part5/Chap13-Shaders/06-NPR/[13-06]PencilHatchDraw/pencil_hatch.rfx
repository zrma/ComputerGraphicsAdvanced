    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D  mapContour;
sampler2D  mapNoise;
float      bumpiness;
float      delta;

struct PS_INPUT 
{
   float2 uv : TEXCOORD0;
};

float4 ps_main( PS_INPUT Input ) : COLOR0
{
   float2 uv = float2( Input.uv.x - delta, Input.uv.y );
   float3 normal;
   
   normal = tex2D(mapNoise, uv);
   
   float4 base = tex2D(mapContour, Input.uv.xy + bumpiness * normal.xy);
   
   return( base + delta );
   
}
